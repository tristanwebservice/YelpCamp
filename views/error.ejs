<% layout("layouts/boilerplate") %>
<div class="row">
  <div class="col-6 offset-3">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading">
        <%= err?.message || "Something went wrong" %>
      </h4>
      <% if (process.env.NODE_ENV !== "production" && err?.stack) { %>
        <pre><%= err.stack %></pre>
      <% } %>
    </div>
  </div>
</div>
